if(<%= @old_packet %>) {
  li = $("li#packet_<%= @old_packet %>").clone();
  li.attr('id', 'packet_<%= @packet.id %>'); }
else {
  li = $("li#submission_<%= @submission.id %>");
  li.attr('id', "packet_<%= @packet.id %>");
  li.addClass('packet').removeClass('submission');
}
meeting = $("section#meeting_<%= @meeting.id %> ol.packets");
li.appendTo(meeting);
